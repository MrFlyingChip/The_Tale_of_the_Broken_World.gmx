    image_speed = .3;
}
if (up_key){
    phy_position_y -= spd;
    view = 4;
    sprite_index = spr_player_run_up;
    image_speed = .3;
}

if(!right_key &amp;&amp; !left_key &amp;&amp; !down_key &amp;&amp; !up_key){
    if(view == 1){
        sprite_index = spr_player_stay_right;
        image_speed = .1;
    }
    if(view == 2){
        sprite_index = spr_player_stay_down;
        image_speed = .1;
    }
